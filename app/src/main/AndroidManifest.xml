<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.project.stickysoft.agecalculator">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AgeResult2"
            android:label="@string/title_activity_age_result2"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".GuardianAge2"
            android:label="@string/title_activity_guardian_age2"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MyAge2"
            android:label="@string/title_activity_my_age2"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ParentResult2"
            android:label="@string/title_activity_parent_result2"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MainActivity2"
            android:label="@string/title_activity_main2"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>